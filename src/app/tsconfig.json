{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es6", "es2015", "dom"],
    "noEmit": true,
    "strict": false,
    "types": ["node", "jest"],
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react"
  },
  "include": ["./**/*.ts", "./**/*.tsx"]
}
